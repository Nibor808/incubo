    "parallelshell": "^2.0.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
     "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-watch": "^2.1.14",
    "file-loader": "^0.9.0",
    "rimraf": "^2.6.1",
    "webpack-dev-middleware": "^1.9.0",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "json-loader": "^0.5.4",
     "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "error": "webpack --display-error-details --config ./config/webpack.config.js",
    "dev": "./bin/dev.sh",
    "mk-images": "mkdir -p ./public/build",
    "cp-images": "mkdir -p ./public/build && cp -R ./src/images ./public/build/images",
    "cp-public": "cp -R ./public ./dist",
    "cp-env": "mkdir -p ./dist/config && cp ./config/.env ./dist/config",
    "cp-views": "cp -R ./src/views ./dist",
    "cp-package": "cp package.json ./dist",
    "mv-app": "mv ./dist/src/* ./dist && rmdir ./dist/src",
    "cp-robots": "cp ./robots.txt ./dist/public",
    "cp-google": "cp ./googleba44b790268a8dc3.html ./dist/public",
    "cp-sitemap": "cp ./sitemap.xml .dist/public",
    "webpack-build": "webpack --config config/webpack.config.js -p",
    "babel-routes": "babel -d ./dist/routes src/routes -s",
    "babel-utils": "babel -d ./dist/utils src/utils -s",
    "babel-app": "babel -d ./dist/ src/app.js -s",
    "babel-build": "npm run babel-routes && npm run babel-utils && npm run babel-app",
    "rimraf-dist": "rimraf ./dist",
    "rimraf-public": "rimraf ./public/build",
    "build-prod": "npm run rimraf-dist && npm run rimraf-public && npm run cp-images && npm run webpack-build && npm run babel-build && npm run cp-public && npm run cp-env && npm run cp-package && npm run cp-views && npm run mv-app && npm run cp-robots && npm run cp-google && npm run cp-sitemap",
    "start": "babel-node --harmony src/app.js"
  },
