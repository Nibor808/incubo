  "main": "app.js",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.3.0",
